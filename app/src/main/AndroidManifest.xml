<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.ui">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name=".TabActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"></activity>
        <activity
            android:name=".MainActivity_First"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".MainActivity_Second"
            android:label="@string/app_name"></activity>
        <activity android:name=".MainActivity_Third"></activity>
        <activity android:name=".HomeworkDetail"></activity>
        <activity android:name=".HomeworkPublish" />
        <activity android:name=".ClassActivity"></activity>
        <activity android:name=".EditUserInfos"></activity>
        <activity android:name=".AddCourseTableActivity"></activity>
        <activity android:name=".WeeklyHomeworks"></activity>
    </application>

</manifest>
